<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pocketguard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PocketGuard">
        <activity android:name=".PaymentActivity" />
        <activity android:name=".EditActivity" />
        <activity android:name=".UserActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".detail2Activity3" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".HimuActivity3" />
        <activity android:name=".MainActivity2" />
        <activity android:name=".OrderActivity" />
        <activity android:name=".CartActivity" />
        <activity android:name=".googleActivity2" />
        <activity android:name=".googleSignin" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".Log_In" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>